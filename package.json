{
  "name": "fastfast-proto",
  "version": "1.0.0",
  "description": "GRPC protobuf files and generated documents",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist/**/*.js",
    "dist/**/*.d.ts",
    "dist/**/*.js.map",
    "proto/**/*"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/vervana-io/ff-proto.git"
  },
  "scripts": {
    "build": "tsc",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "prepublishOnly": "npm run build",
    "clean": "rm -rf lib/*.js lib/*.d.ts lib/*.js.map"
  },
  "keywords": [
    "grpc",
    "protobuff",
    "microservice"
  ],
  "author": "Akeeb Ismail",
  "license": "ISC",
  "devDependencies": {
    "@types/jest": "^30.0.0",
    "@types/node": "^24.1.0",
    "jest": "^30.0.5",
    "nestjs-proto-gen-ts": "^1.0.21",
    "ts-jest": "^29.4.0",
    "ts-node": "^10.9.2",
    "ts-proto": "^2.7.7",
    "typescript": "^5.8.3"
  },
  "peerDependencies": {
    "@nestjs/common": "^11.1.3",
    "@nestjs/microservices": "^11.1.3"
  },
  "dependencies": {
    "app-root-path": "^3.1.0"
  }
}
